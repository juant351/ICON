    <!-- http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>AmericanaPizza esta definida por tener Mozzarella, Tomato , Pepperonni and
Jalappeno topping</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#AmericanaPizza -->

