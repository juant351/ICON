                <owl:onProperty rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


