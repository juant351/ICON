    <!-- http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/juan_/ontologies/2022/0/PizzaRepaso#GreenPepperTopping">
